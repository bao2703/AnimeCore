@inject IMovieService MovieService
@{
    ViewData["Title"] = "Home Page";
}

@section slide {
    @await Html.PartialAsync("_SlidePartial")
}

@await Component.InvokeAsync("MovieSlide", new {model = MovieService.FindPopularMovie(16), title = "Popular"})

@await Component.InvokeAsync("MovieSlide", new {model = MovieService.FindNewestMovie(16), title = "Newest"})